# AWS Secrets Manager ARN for Teams webhook URL
TEAMS_WEBHOOK_URL_SECRET_ARN=arn:aws:secretsmanager:us-east-1:123456789012:secret:teams-webhook-url

# AWS Secrets Manager ARN for Bitbucket webhook secret
BITBUCKET_SECRET_ARN=arn:aws:secretsmanager:us-east-1:123456789012:secret:bitbucket-webhook-secret

# Enable IP restriction validation (optional, defaults to true)
IP_RESTRICTION_ENABLED=true

# Bitbucket IP ranges (optional, comma-separated CIDR notation)
# If not set, uses default Bitbucket Cloud IP ranges
# Example with custom ranges:
BITBUCKET_IP_RANGES=104.192.136.0/21,185.166.140.0/22,13.200.41.128/25
